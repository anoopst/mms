	<queryString>
		<![CDATA[Select itm.IT_NAME,((itm.IT_STOCK + (Select IFNULL(SUM(gid.GID_QTY),0) from mm_goodsissuedetail gid, mm_goodsissueheader gih Where gih.GIH_NO = gid.GIH_NO and gid.IT_NO = itm.IT_NO ) - (Select IFNULL(SUM(grd.GRH_QTY),0) from mm_goodsreceiptdetail grd, mm_goodsreceiptheader grh Where grh.GRH_NO = grd.GRH_NO and grd.IT_NO = itm.IT_NO)) +((Select IFNULL(SUM(grd.GRH_QTY),0) from mm_goodsreceiptdetail grd, mm_goodsreceiptheader grh Where grh.GRH_NO = grd.GRH_NO and grd.IT_NO = itm.IT_NO and grh.GRH_DATE < $P{FromDate}) - (Select IFNULL(SUM(gid.GID_QTY),0) from mm_goodsissuedetail gid, mm_goodsissueheader gih Where gih.GIH_NO = gid.GIH_NO and gid.IT_NO = itm.IT_NO and gih.GIH_DATE < $P{FromDate}))) as IT_STOCK,(Select IFNULL(SUM(grd.GRH_QTY),0) from mm_goodsreceiptdetail grd, mm_goodsreceiptheader grh Where grh.GRH_NO = grd.GRH_NO and grd.IT_NO = itm.IT_NO and grh.GRH_DATE >= $P{FromDate} and grh.GRH_DATE <= $P{ToDate}) as RECEIPTQTY,(Select IFNULL(SUM(gid.GID_QTY),0) from mm_goodsissuedetail gid, mm_goodsissueheader gih Where gih.GIH_NO = gid.GIH_NO and gid.IT_NO = itm.IT_NO and gih.GIH_DATE >= $P{FromDate} and gih.GIH_DATE <= $P{ToDate}) as ISSUEQTY , uom.UM_NAME From mm_item itm,mm_uom uom Where itm.UM_NO =uom.UM_NO and itm.IT_NO = $P{itemno}]]>
	</queryString>
